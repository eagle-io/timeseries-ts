{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://github.com/eagle-io/timeseries/blob/master/jts.schema.json",
  "type": "object",
  "default": {},
  "title": "JSON TimeSeries",
  "$defs": {
    "columnHeader": {
      "type": "object",
      "default": {},
      "title": "Column header",
      "required": [
        "id",
        "name",
        "dataType"
      ],
      "properties": {
        "id": {
          "type": "string",
          "pattern": "^\\w{24}$",
          "default": "",
          "title": "Column id",
          "examples": [
            "541a5a129bc9b4035f906d70"
          ]
        },
        "name": {
          "type": "string",
          "default": "",
          "title": "Column name",
          "examples": [
            "Temperature"
          ]
        },
        "dataType": {
          "enum": ["NUMBER", "TEXT", "TIME", "COORDINATES"],
          "default": "",
          "title": "Column data type",
          "examples": [
            "NUMBER"
          ]
        },
        "renderType": {
          "enum": ["VALUE", "STATE"],
          "default": "",
          "title": "Column render mode",
          "examples": [
            "VALUE"
          ]
        },
        "format": {
          "type": "string",
          "default": "0.###",
          "title": "Column format",
          "examples": [
            "0.###"
          ]
        },
        "aggregate": {
          "enum": ["NONE", "INTERPOLATED", "AVERAGE", "MEDIAN", "TOTAL", "MIN", "MAX", "RANGE", "DELTA", "CHANGE", "START", "END", "COUNT"],
          "default": "",
          "title": "Column aggregate",
          "examples": [
            "NONE"
          ]
        }
      },
      "examples": [{
        "id": "541a5a129bc9b4035f906d70",
        "name": "Temperature",
        "dataType": "NUMBER",
        "renderType": "VALUE",
        "format": "0.###",
        "aggregate": "NONE"
      }]
    },
    "columnData": {
      "type": "object",
      "title": "Column data",
      "required": [],
      "properties": {
        "v": {
          "type": [
            "integer",
            "number",
            "string"
          ],
          "title": "Data value",
          "examples": [
            99,
            28.22,
            29.2,
            29.18
          ]
        },
        "q": {
          "type": "integer",
          "title": "Data quality",
          "examples": [
            100
          ]
        },
        "a": {
          "type": "string",
          "title": "Data annotation",
          "examples": [
            "site maintenance",
            "sensor recalibrated"
          ]
        }
      },
      "examples": [{
        "v": 99,
        "q": 100,
        "a": "site maintenance"
      },
        {
          "v": 28.22
        },
        {
          "a": "sensor recalibrated"
        },
        {
          "v": 29.2,
          "q": 100
        },
        {
          "v": 29.18
        }]
    }
  },
  "required": [
    "docType",
    "version",
    "header",
    "data"
  ],
  "properties": {
    "docType": {
      "type": "string",
      "default": "jts",
      "title": "Document format",
      "examples": [
        "jts"
      ]
    },
    "version": {
      "type": "string",
      "default": "1.0",
      "title": "Document format version",
      "examples": [
        "1.0"
      ]
    },
    "header": {
      "type": "object",
      "default": {},
      "title": "Document header",
      "required": [
        "startTime",
        "endTime",
        "recordCount",
        "columns"
      ],
      "properties": {
        "startTime": {
          "type": "string",
          "format": "date-time",
          "default": "",
          "title": "First timestamp in data range",
          "examples": [
            "2018-08-16T02:00:00.000Z"
          ]
        },
        "endTime": {
          "type": "string",
          "format": "date-time",
          "default": "",
          "title": "Last timestamp in data range",
          "examples": [
            "2018-08-16T02:20:43.000Z"
          ]
        },
        "recordCount": {
          "type": "integer",
          "default": 0,
          "title": "Number of records in data range",
          "examples": [
            5
          ]
        },
        "columns": {
          "type": "object",
          "default": {},
          "title": "Column headers",
          "required": [
            "0"
          ],
          "patternProperties" :{
            "^\\d+$": {
              "$ref": "#/$defs/columnHeader"
            }
          },
          "examples": [{
            "0": {
              "id": "541a5a129bc9b4035f906d70",
              "name": "Temperature",
              "dataType": "NUMBER",
              "renderType": "VALUE",
              "format": "0.###",
              "aggregate": "NONE"
            }
          }]
        }
      },
      "examples": [{
        "startTime": "2018-08-16T02:00:00.000Z",
        "endTime": "2018-08-16T02:20:43.000Z",
        "recordCount": 5,
        "columns": {
          "0": {
            "id": "541a5a129bc9b4035f906d70",
            "name": "Temperature",
            "dataType": "NUMBER",
            "renderType": "VALUE",
            "format": "0.###",
            "aggregate": "NONE"
          }
        }
      }]
    },
    "data": {
      "type": "array",
      "default": [],
      "title": "Data records",
      "items": {
        "type": "object",
        "title": "A Schema",
        "required": [
          "ts",
          "f"
        ],
        "properties": {
          "ts": {
            "type": "string",
            "format": "date-time",
            "title": "Record timestamp",
            "examples": [
              "2018-08-16T02:00:39.000Z",
              "2018-08-16T02:05:40.000Z",
              "2018-08-16T02:10:41.000Z",
              "2018-08-16T02:15:42.000Z",
              "2018-08-16T02:20:43.000Z"
            ]
          },
          "f": {
            "type": "object",
            "title": "Record fields",
            "required": [],
            "patternProperties" :{
              "^\\d+$": {
                "$ref": "#/$defs/columnData"
              }
            },
            "examples": [{
              "0": {
                "v": 99,
                "q": 100,
                "a": "site maintenance"
              }
            },
              {
                "0": {
                  "v": 28.22
                }
              },
              {
                "0": {
                  "a": "sensor recalibrated"
                }
              },
              {
                "0": {
                  "v": 29.2,
                  "q": 100
                }
              },
              {
                "0": {
                  "v": 29.18
                }
              }]
          }
        },
        "examples": [{
          "ts": "2018-08-16T02:00:39.000Z",
          "f": {
            "0": {
              "v": 99,
              "q": 100,
              "a": "site maintenance"
            }
          }
        },
          {
            "ts": "2018-08-16T02:05:40.000Z",
            "f": {
              "0": {
                "v": 28.22
              }
            }
          },
          {
            "ts": "2018-08-16T02:10:41.000Z",
            "f": {
              "0": {
                "a": "sensor recalibrated"
              }
            }
          },
          {
            "ts": "2018-08-16T02:15:42.000Z",
            "f": {
              "0": {
                "v": 29.2,
                "q": 100
              }
            }
          },
          {
            "ts": "2018-08-16T02:20:43.000Z",
            "f": {
              "0": {
                "v": 29.18
              }
            }
          }]
      },
      "examples": [
        [{
          "ts": "2018-08-16T02:00:39.000Z",
          "f": {
            "0": {
              "v": 99,
              "q": 100,
              "a": "site maintenance"
            }
          }
        },
          {
            "ts": "2018-08-16T02:05:40.000Z",
            "f": {
              "0": {
                "v": 28.22
              }
            }
          },
          {
            "ts": "2018-08-16T02:10:41.000Z",
            "f": {
              "0": {
                "a": "sensor recalibrated"
              }
            }
          },
          {
            "ts": "2018-08-16T02:15:42.000Z",
            "f": {
              "0": {
                "v": 29.2,
                "q": 100
              }
            }
          },
          {
            "ts": "2018-08-16T02:20:43.000Z",
            "f": {
              "0": {
                "v": 29.18
              }
            }
          }]
      ]
    }
  },
  "examples": [{
    "docType": "jts",
    "version": "1.0",
    "header": {
      "startTime": "2018-08-16T02:00:00.000Z",
      "endTime": "2018-08-16T02:20:43.000Z",
      "recordCount": 5,
      "columns": {
        "0": {
          "id": "541a5a129bc9b4035f906d70",
          "name": "Temperature",
          "dataType": "NUMBER",
          "renderType": "VALUE",
          "format": "0.###",
          "aggregate": "NONE"
        }
      }
    },
    "data": [{
      "ts": "2018-08-16T02:00:39.000Z",
      "f": {
        "0": {
          "v": 99,
          "q": 100,
          "a": "site maintenance"
        }
      }
    },
      {
        "ts": "2018-08-16T02:05:40.000Z",
        "f": {
          "0": {
            "v": 28.22
          }
        }
      },
      {
        "ts": "2018-08-16T02:10:41.000Z",
        "f": {
          "0": {
            "a": "sensor recalibrated"
          }
        }
      },
      {
        "ts": "2018-08-16T02:15:42.000Z",
        "f": {
          "0": {
            "v": 29.2,
            "q": 100
          }
        }
      },
      {
        "ts": "2018-08-16T02:20:43.000Z",
        "f": {
          "0": {
            "v": 29.18
          }
        }
      }]
  }]
}
